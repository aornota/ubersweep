{
    "editor.fontSize": 13,
    "tailwindCSS.includeLanguages": {
        "fsharp": "html"
    },
    "tailwindCSS.experimental.classRegex": [
        "\\.className\\s+\"([^\"]*)\"",
        ["\\.className\\s+\\[([^\\]]*)\\]", "\"([^\"]*)\""]
    ],
    "files.exclude": {
        "**/.editorconfig": true,
        "**/.fantomasignore": true,
        "**/.git": true,
        "**/.npmrc": true,
        "**/.paket": true,
        "**/.vscode/extensions.json": true,
        "**/Application.sln": true,
        "**/bin": true,
        "**/deploy": true,
        "**/LICENSE": true,
        "**/node_modules": true,
        "**/obj": true,
        "**/output": true,
        "**/package-lock.json": true,
        "**/paket.lock": true,
        "**/paket-files": true,
        "**/persisted": true,
        "**/secret": true,
    },
    "[fsharp]": {
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "Ionide.Ionide-fsharp"
    }
}
